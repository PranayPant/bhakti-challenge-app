{
  "expo": {
    "name": "bhakti-challenge-app",
    "slug": "bhakti-challenge-app",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/murali.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.punch.up.bhaktichallengeapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/murali.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.punch.up.bhaktichallengeapp",
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/murali.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/murali.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-asset",
        {
          "assets": ["./assets/images/"]
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/NotoSansDevanagari-Regular.ttf"]
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9c69e482-db65-44a4-ac77-7b2273852cdd"
      }
    }
  }
}
